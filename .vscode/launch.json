            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal",
            "args": ["--mode", "both",
                    "--epoch", "10",
                    "--lr", "0.00005",
                    "--inter_attn_method", "general",
                    "--sqlfile", "HNAE/SQL/cloth_interaction@6_teston@15.sql",
                    "--having_interactions", "6",
                    "--num_of_rating", "1",
                    "--num_of_reviews" , "5",
                    "--user_pretrain_wordVec", "N",
                    "--use_nltk_stopword","N",
